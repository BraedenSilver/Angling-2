{
  "schemaVersion": 1,
  "id": "waterlogged",
  "version": "${version}",

  "name": "Waterlogged Mod",
  "description": "Expands and deepens the aquatic experience with new creatures, plants, and ocean structures.",
  "authors": [
    "Braeden Silver",
    "EightSidedSquare",
    "Diansu",
    "Sillvia"
  ],
  "contact": {
    "homepage": "https://github.com/BraedenSilver/Waterlogged"
  },

  "license": "CC0-1.0",
  "icon": "assets/waterlogged/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.braeden.waterlogged.WaterloggedMod"
    ],
    "client": [
      "net.braeden.waterlogged.WaterloggedClient"
    ],
    "fabric-datagen": [
      "net.braeden.waterlogged.WaterloggedDataGenerator"
    ]
  },
  "mixins": [
    "waterlogged.mixins.json"
  ],
  "accessWidener": "waterlogged.accesswidener",

  "depends": {
    "fabricloader": ">=0.18.0",
    "fabric-api": "*",
    "minecraft": "~1.21",
    "java": ">=21"
  }
